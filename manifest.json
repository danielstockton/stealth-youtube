{
  "manifest_version": 2,

  "name": "Stealth Youtube",
  "description": "Listen to Youtube videos without anyone seeing what it is.",
  "version": "0.1",
  "permissions": [
    "*://www.youtube.com/*",
    "storage"
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["*://www.youtube.com/*"],
      "js": ["jquery.js", "stealth-youtube.js"],
      "run_at": "document_end"
    }
  ]
}
